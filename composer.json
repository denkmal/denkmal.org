{
  "name": "denkmal/denkmal.org",
  "config": {
    "bin-dir": "bin/"
  },
  "require": {
    "cargomedia/cm": "~1.205.0",
    "ricardoper/twitteroauth": "~2.1.6",
    "nojimage/twitter-text-php": "~1.1.1",
    "codemonkeys-ru/gcm-message": "~0.2.0",
    "guzzlehttp/guzzle": "~5.3.0",
    "facebook/graph-sdk": "~5.3.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.5.0",
    "tomaszdurka/mocka": "~0.12.0",
    "fzaninotto/faker": "~1.5.0"
  },
  "minimum-stability": "alpha",
  "repositories": [
    {
      "type": "composer",
      "url": "https://satis.cargomedia.ch/dist"
    }
  ],
  "autoload": {
    "psr-0": {
      "Denkmal_": "library/Denkmal/library/",
      "Admin_": "library/Admin/library/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "DenkmalTest_": "tests/helpers/",
      "CMTest_": "vendor/cargomedia/cm/tests/helpers/CMTest/library/"
    }
  },
  "extra": {
    "cm-modules": {
      "Denkmal": {
        "path": "library/Denkmal/"
      },
      "Admin": {
        "path": "library/Admin/"
      }
    }
  }
}
